  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'rmdev'",
      "cargo": {
        "args": ["build", "--bin=rmdev", "--package=rmdev"],
        "filter": {
          "name": "rmdev",
          "kind": "bin"
        }
      },
      "args": ["clear", ".."],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'rmdev'",
      "cargo": {
        "args": ["test", "--no-run", "--bin=rmdev", "--package=rmdev"],
        "filter": {
          "name": "rmdev",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
